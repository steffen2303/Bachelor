@MISC{TOTEM05,
  author = {S. Balon and J. Lepropre and G. Monfort},
  title = {{TOTEM 2.0 -- TOolbox for Traffic Engineering Methods}},
  howpublished = {{http://totem.run.montefiore.ulg.ac.be}},
  year = {2005},
  abstract = {We present the software architecture of an open source Traffic Engineering
